<%= form_for @entry do |f| %>

	<!-- error checking needed. -->

	<div>
		title: <%= f.text_field :title %>
		<br>
		body: <%= f.text_area :body, size: "60x12" %> 
		<br>
		author_id: <%= f.hidden_field :author_id, value: " #{current_user.id}"  %>
		<br>
		<%= f.submit "Submit Entry!" %>

	</div>


<% end %>